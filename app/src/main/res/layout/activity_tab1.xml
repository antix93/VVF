<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.tizi.vvf.Tab1">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Intervento"
        android:id="@+id/textView10"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="X"
        android:id="@+id/NumInt"
        android:layout_alignTop="@+id/textView10"
        android:layout_toEndOf="@+id/textView10"
        android:layout_marginStart="31dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Motivo \n Intervento"
        android:id="@+id/MotIntBtn"
        android:layout_marginTop="31dp"
        android:width="140dp"
        android:height="100dp"
        android:layout_below="@+id/textView10"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Enti \n Intervenuti"
        android:id="@+id/EntIntBtn"
        android:layout_alignBottom="@+id/MotIntBtn"
        android:layout_alignParentEnd="true"
        android:width="140dp"
        android:height="100dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Personale \n Intervenuto"
        android:id="@+id/PerIntBtn"
        android:width="140dp"
        android:height="100dp"
        android:layout_above="@+id/OraBtn"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="39dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mezzi \n Intervenuti"
        android:id="@+id/MezIntBtn"
        android:layout_alignBottom="@+id/PerIntBtn"
        android:layout_alignEnd="@+id/EntIntBtn"
        android:width="140dp"
        android:height="100dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Orari \n Uscita/Rientro"
        android:id="@+id/OraBtn"
        android:height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/MezIntBtn"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
